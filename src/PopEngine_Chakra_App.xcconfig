//	config for OSX bundle apps
//#include "PopApp.xcconfig"


GCC_PREPROCESSOR_DEFINITIONS = JSAPI_CHAKRA $(inherited) TARGET_OSX_BUNDLE ENABLE_OPENGL
INFOPLIST_FILE = $(SRCROOT)/Info.plist

CHAKRA_PATH = $(SRCROOT)/src/Libs/ChakraCore/include
BROADWAY_PATH = $(SRCROOT)/src/SoyBroadway/Broadway/Decoder
SOYLIB_PATH = $(SRCROOT)/src/
HEADER_SEARCH_PATHS = $(HEADER_SEARCH_PATHS) $(inherited) $(CHAKRA_PATH) $(DLIB_PATH)/include $(BROADWAY_PATH)/inc $(SOYLIB_PATH)
LIBRARY_SEARCH_PATHS = $(LIBRARY_SEARCH_PATHS) $(inherited) $(DLIB_PATH)/lib

OTHER_CFLAGS = -Wno-ambiguous-macro

//	10.13 for coreml
MACOSX_DEPLOYMENT_TARGET = 10.13
